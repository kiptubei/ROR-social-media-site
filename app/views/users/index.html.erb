<section class="users-section">
  <ul class="users-list">
    <% @users.each do |user| %>
        <li>
          Name: <%= user.name %>
          <span class="profile-link">
            <%= link_to 'See Profile',  user_path(user), class: 'profile-link' %>
            <%= display_friend_invite_button(user) %>
          </span>
        </li>
    <% end %>
  </ul>
</section>
